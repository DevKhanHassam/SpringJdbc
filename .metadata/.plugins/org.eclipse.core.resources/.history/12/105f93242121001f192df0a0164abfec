package com.Jdbc.com.Jdbc.Practice;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import DAO.Query;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
        
        Student_DTO student = new Student_DTO();
        
        student.setId(0);
        student.setName("Ali");
        student.setAddress("Hause I");
        
        
        ApplicationContext context = new ClassPathXmlApplicationContext("com/Jdbc/com/Jdbc/Practice/configure.xml");      
       Query bean = context.getBean("Query", Query.class);
       
       int insert = bean.insert(student);
        
       System.out.println("update sucessfull "+ insert);
    
       
       ((ClassPathXmlApplicationContext)context).close();
        
    }
}
